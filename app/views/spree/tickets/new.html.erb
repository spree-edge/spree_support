<%= render 'spree/shared/error_messages', target: @ticket %>
<div class="col-lg-6 offset-lg-3">
  <h1 class="spree-header spree-mb-large spree-mt-large"><%= 'Create Ticket' %></h1>
  <%= form_for @ticket, html: { id: 'checkout_form_address' },  multipart: true do |f| %>
    <%= render 'form', f: f %>

    <%= f.submit 'Create', class: 'btn btn-primary btn-block spree-btn mb-5' %>

    <%= link_to 'Cancel', tickets_path , class: 'btn-outline-dark btn btn-block spree-btn mb-5 text-20' %>
  <% end %>
</div>
